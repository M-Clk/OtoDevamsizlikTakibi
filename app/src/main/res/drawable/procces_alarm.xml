<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/backround">
        <shape>
            <gradient android:startColor="#00ffffff"/>
            <padding android:left="5dp" android:right="5dp" android:bottom="5dp"/>
        </shape>
    </item>
    <item android:id="@+id/procces">
        <clip>
            <shape>
                <gradient android:endColor="@color/black_overlay"/>
                <padding android:left="5dp" android:right="5dp" android:bottom="5dp"/>
                <corners android:topRightRadius="11dp" android:bottomRightRadius="11dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
